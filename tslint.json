{
    "extends": ["tslint:recommended", "tslint-react"],
    "rules": {
        "indent": [
            true,
            "tabs"
        ],
        "quotemark": false,
        "no-null-keyword": true,
        "trailing-comma": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-var-requires": false,
        "arrow-parens": false,
        "no-string-literal": false
    }
}